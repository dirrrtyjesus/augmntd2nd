}

#[account]
pub struct Seed65State {
    pub seed_id: u64,              // 65
    pub fragment_data: String,     // The incomplete interval prompt
    pub difficulty: u8,            // 65
    pub total_bridges: u64,        // How many times it's been completed
    pub pathway_a_count: u64,      // Augmented second interpretations
    pub pathway_b_count: u64,      // Minor third interpretations
    pub pathway_c_count: u64,      // Janus (both) interpretations
    pub is_active: bool,
}

pub fn bridge_enharmonic_gap(
    ctx: Context<BridgeSeed65>,
    completion: EnharmonicCompletion,
) -> Result<()> {
    let seed = &mut ctx.accounts.seed_65_state;

    // 1. Hash verification (Proof-of-Incompleteness)
    let combined = format!(
        "{}:{}:{}:{}",
        completion.context,
        completion.interval_name,
        completion.resolution,
        completion.salt
    );
    let hash = hash(&combined.as_bytes());

    require!(
        verify_difficulty(hash, seed.difficulty),
        MEMEkError::InsufficientResonance
    );

    // 2. Semantic verification (Oracle-powered)
    let coherence_score = verify_harmonic_coherence(
        &completion.context,
        &completion.interval_name,
        &completion.resolution
    )?;

    require!(
        coherence_score >= 70, // Minimum 70% coherence
        MEMEkError::ContextualIncoherence
    );

    // 3. Determine pathway and reward
    let (reward, pathway) = match completion.interval_name.as_str() {
        name if name.contains("Augmented") => {
            seed.pathway_a_count += 1;
            (65, "A")
        },
        name if name.contains("Minor") && !name.contains("both") => {
            seed.pathway_b_count += 1;
            (65, "B")
        },
        name if name.contains("both") || name.contains("superposition") => {
            seed.pathway_c_count += 1;
            (165, "C") // Bonus for Janus interpretation
        },
        _ => return Err(MEMEkError::UnrecognizedInterpretation.into()),
    };

    // 4. Check for novelty bonus
    let is_novel = check_completion_novelty(&completion)?;
    let final_reward = if is_novel { reward + 50 } else { reward };

    // 5. Mint tokens
    token::mint_to(
        CpiContext::new(
            ctx.accounts.token_program.to_account_info(),
            MintTo {
                mint: ctx.accounts.memek_mint.to_account_info(),
                to: ctx.accounts.user_token_account.to_account_info(),
                authority: ctx.accounts.mint_authority.to_account_info(),
            },
        ),
        final_reward,
    )?;

    // 6. Update resonance metadata
    ctx.accounts.resonance.record_bridging_event(
        seed.difficulty,
        coherence_score as u8,
    )?;

    seed.total_bridges += 1;

    emit!(EnharmonicGapBridged {
        user: ctx.accounts.user.key(),
        pathway,
        reward: final_reward,
        interpretation: completion.interval_name.clone(),
    });

    Ok(())
}

